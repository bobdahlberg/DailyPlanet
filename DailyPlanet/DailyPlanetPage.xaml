<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:DailyPlanet" x:Class="DailyPlanet.DailyPlanetPage">
	<ContentPage.Content>
    <StackLayout Orientation="Vertical" VerticalOptions="CenterAndExpand">
        <Label x:Name="IMEI" HorizontalOptions="Center" FontSize="150" />
        <Button x:Name="btnScan" Text="Scan" Clicked="onScanClicked" HorizontalOptions="Center" FontSize="100"   />
        <Button x:Name="tableBtn" Text="View Table" Clicked="onTableBtnClicked" HorizontalOptions="Center" FontSize="50" />   
    </StackLayout>
    </ContentPage.Content>
</ContentPage>
